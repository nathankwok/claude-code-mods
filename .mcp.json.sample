{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "codex": {
      "type": "stdio",
      "command": "codex",
      "args": [
        "--profile",
        "gpt_5_high_codex",
        "mcp"
      ],
      "env": {}
    },
    "clear-thought": {
      "command": "npx",
      "args": [
        "-y",
        "@waldzellai/clear-thought"
      ]
    }
  }
}
